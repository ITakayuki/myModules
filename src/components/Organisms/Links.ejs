<%
    github = typeof github !== "undefined"? github : null;
    npm = typeof npm !== "undefined"?  npm : null;
    qiita = typeof qiita !== "undefined"? qiita:null;
 %>

<div class="links">
    <div links-css="wrap">
        <% if (github) { %>
        <div links-css="iconWrap">
            <a href="<%= github %>" target="_blank">
                <img src="/assets/img/common/icon-github.svg" alt="Github">
            </a>
        </div>
        <%}%>
        <% if (npm) {%>
        <div links-css="iconWrap">
            <a href="<%= npm %>" target="_blank">
                <img src="/assets/img/common/icon-npm.svg" alt="npm">
            </a>
        </div>
            <%}%>
        <% if (qiita) { %>
        <div links-css="iconWrap">
            <a href="<%= qiita %>" target="_blank">
                <img src="/assets/img/common/icon-qiita.png" alt="npm">
            </a>
        </div>
        <%}%>
    </div>
</div>